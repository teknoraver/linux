&mipi_csi2_0 {
	status = "okay";
	num_lanes = <4>;
	snps,en-ppi-width = <1>; // 1: 16-bit, 0: 8-bit
	ports {
		#address-cells = <1>;
		#size-cells = <0>;

		port@0 {
			#address-cells = <1>;
			#size-cells = <0>;	
			reg = <0>;
			mipi_csi2_input: endpoint {
				remote-endpoint = <&csidphy0_out>;
				data-lanes = <1 2>;
			};
		};

		port@1 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <1>;
			mipi_csi2_output: endpoint {
				remote-endpoint = <&axi2dvp_mipi2_in0>;
			};
		};

		port@2 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <2>;
			mipi_csi2_output1: endpoint {
				remote-endpoint = <&vvcam_isp_input_port>;
			};
		};
	};
};

